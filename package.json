{
  "name": "medcare-solutions",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "setup": "echo 'No setup required'",
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-webstorage\" next build",
    "dev": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-webstorage\" next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-webstorage\" next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-webstorage\" payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-webstorage\" payload generate:types",
    "lint": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-webstorage\" next lint",
    "lint:fix": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-webstorage\" next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "payload": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-webstorage\" payload",
    "start": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-webstorage\" next start",
    "clean": "rm -rf .next && rm -rf node_modules && pnpm install",
    "type-check": "tsc --noEmit",
    "validate": "pnpm lint && pnpm format:check && pnpm type-check"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.840.0",
    "@hookform/resolvers": "^5.1.1",
    "@payloadcms/db-postgres": "3.76.1",
    "@payloadcms/next": "3.76.1",
    "@payloadcms/payload-cloud": "3.76.1",
    "@payloadcms/richtext-lexical": "3.76.1",
    "@payloadcms/storage-s3": "3.76.1",
    "@payloadcms/ui": "3.76.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/postcss": "^4.1.10",
    "@tanstack/react-query": "^5.81.2",
    "@tanstack/react-table": "^8.21.3",
    "canvas": "^3.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "graphql": "^16.8.1",
    "html2canvas": "^1.4.1",
    "jsdom": "^26.1.0",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.522.0",
    "next": "15.3.9",
    "payload": "3.76.1",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.58.1",
    "sharp": "0.34.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.9",
    "ignore-loader": "^0.1.2",
    "prettier": "^3.4.2",
    "tw-animate-css": "^1.3.4",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=22",
    "pnpm": "^9"
  },
  "packageManager": "pnpm@9.15.9",
  "pnpm": {
    "onlyBuiltDependencies": [
      "canvas",
      "sharp"
    ]
  }
}
